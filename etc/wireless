SSID=mango_test
Password=iotempire


="mango_test"
="iotempire"

# /etc/config/wireless

config wifi-device 'mt7628'
        option type 'mt7628'
        option ht_txstream '2'
        option ht_rxstream '2'
        option hwmode '11a'
        option noscan '0'
        option txpower '20'
        option txpower_max '20'
        option country 'US'
        option region '0'
        option band '2G'
        option mimops '3'
        option wmm '1'
        option disabled '1'

config wifi-device 'radio0'
        option type 'mac80211'
        option path 'platform/10300000.wmac'
        option channel '11'
        option band '2g'
        option htmode 'HT20'
        option cell_density '0'

config wifi-iface 'default_radio0'
        option device 'radio0'
        option network 'lan'
        option mode 'ap'
        option ssid 'mango_test'
        option encryption 'psk2'
        option key 'iotempire'

